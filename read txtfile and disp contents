#include <stdio.h>

int main() {
    FILE *fp;
    char ch;

    // Open the file in read mode
    fp = fopen("example.txt", "r");

    // Check if the file is opened successfully
    if (fp == NULL) {
        printf("Error opening file\n");
        return 1;
    }

    // Read and display the contents of the file
    while ((ch = fgetc(fp)) != EOF) {
        printf("%c", ch);
    }

    // Close the file
    fclose(fp);

    return 0;
}
